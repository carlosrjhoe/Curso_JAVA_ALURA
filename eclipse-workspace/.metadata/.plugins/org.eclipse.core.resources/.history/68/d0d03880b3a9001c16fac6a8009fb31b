
public class TesteTipos {
	public static void main(String[] args) {
		FuncionarioTeste carlos = new FuncionarioTeste();
		carlos.setNome("carlos");
		carlos.setSalario(2740);
		
		FuncionarioTeste mayara = new FuncionarioTeste();
		mayara.setTipo(2);
		mayara.setNome("Mayara");
		mayara.setSalario(3500);
		
		System.out.println(carlos.getTipo());
		System.out.println(carlos.getNome());
		System.out.println(carlos.getSalario());
		
		System.out.println(mayara.getTipo());
		System.out.println(mayara.getNome());
		System.out.println(mayara.getSalario());
	}
}
