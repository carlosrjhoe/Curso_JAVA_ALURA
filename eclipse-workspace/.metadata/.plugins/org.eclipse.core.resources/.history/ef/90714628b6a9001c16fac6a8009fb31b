
public class TesteTipos {
	public static void main(String[] args) {
		FuncionarioTeste carlos = new FuncionarioTeste();
		carlos.setNome("carlos");
		carlos.setSalario(2740);
		System.out.println(carlos.getTipo());
		System.out.println(carlos.getNome());
		System.out.println(carlos.getBonificacao());
		
		FuncionarioTeste neto = new FuncionarioTeste();
		neto.setTipo(1);
		neto.setNome("Neto");
		neto.setSalario(3000);
		System.out.println(neto.getNome());
		System.out.println(neto.getBonificacao());
		
		FuncionarioTeste mayara = new FuncionarioTeste();
		mayara.setTipo(2);
		mayara.setNome("Mayara");
		mayara.setSalario(4500);
		System.out.println(mayara.getTipo());
		System.out.println(mayara.getNome());
		System.out.println(mayara.getBonificacao());
	}
}
