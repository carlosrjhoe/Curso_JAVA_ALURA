public class TesteGerente {
	public static void main(String[] args) {
		Gerente carlos = new Gerente(); // Construtor
		carlos.setNome("Carlos"); // Atributos extendidos da Class Funcionário
		carlos.setSalario(3500); // Atributos extendidos da Class Funcionário
		carlos.setSenha(1234); // Atributo da Class Gerente
		
		System.out.println(carlos.getNome());
		System.out.println(carlos.getSalario());
		System.out.println(carlos.autentica(1234));
	}
}
