
public class TesteIR {

		public static void main(String[] args) {
			
			double salario = 3150.00;
			
			if (salario < 2600.00) {
				System.out.println("A sua aliquota é de 15%");
				System.out.println("Você pode reduzir até R$350.00");
			}
			if (salario < 3750.00){
				System.out.println("A sua aliquota é de 22,5%");
				System.out.println("Você pode reduzir até R$636.00");
			}
		}
}
